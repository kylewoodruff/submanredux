{
  "name": "submanredux",
  "version": "1.0.0",
  "description": "server side packages",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kylewoodruff/submanredux.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kylewoodruff/submanredux/issues"
  },
  "devDependencies": {
    "concurrently": "^4.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "nodemon": "^1.18.7"
  },
  "homepage": "https://github.com/kylewoodruff/submanredux#readme",
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.1.0",
    "auth0-js": "^9.11.2",
    "apexcharts": "^3.8.2",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "install": "^0.13.0",
    "mongoose": "^5.6.2",
    "react-router-dom": "^5.0.1",
    "styled-components": "^4.3.2"
  }
}
